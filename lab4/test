%{ blalblalba<>llaba %}

%token %<void%> AAA
%token %<void%> BBB

%type %<void%> aaaa[ %<void%> %<int%>]
%type %<void%> bbbb[]
%type %<void%> start[%<void%>]

%%

start:
AAA aaaa [_s1 _s3 _h45] AAA {*&(*&$)#(@*#@Q>}|
AAA aaaa [_s1 _s3 _h45] AAA bbbb {*&(*&$)#(@*#@Q>}|;

aaaa:
AAA bbbb AAA {*&(*&$)#(@*#@Q>}
| bbbb {ksjd;lfks kjfd;lsadkf } |;

bbbb:
BBB { kj;lkj;lkj;lkj }
| {dalskjf;lsdkfj}|;
%%

