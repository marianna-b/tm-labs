%{ blalblalba<>llaba %}

%token %<int%> : _A AAA
%token %<void%> BBB

%type %<int%> aaaa[ %<bool%> %<int%>]
%type %<void%> bbbb[]
%type %<int%> start[%<int%>]

%%

start:
AAA aaaa [_s1 _h1] AAA bbbb BBB ${*&(*&)#(@*#@Q>$}|;

aaaa:
AAA bbbb AAA ${*&(*&)#(@*#@Q>$}
| bbbb ${ksjd;lfks kjfd;lsadkf $} |;

bbbb:
BBB ${ kj;lkj;lkj;lkj $}
| ${dalskjf;lsdkfj$}|;
%%

